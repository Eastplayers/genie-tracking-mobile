# Mobile Tracking SDK - Environment Variables Example
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# =============================================================================
# SDK Configuration (for testing/examples)
# =============================================================================

# Your brand ID from the tracking platform (REQUIRED)
BRAND_ID=your_brand_id_here

# Optional: API key for authenticated requests (REQUIRED)
X_API_KEY=your_api_key_here

# Optional: API endpoint URL (defaults to https://tracking.api.founder-os.ai/api)
# Only set this if you're using a custom backend
API_URL=

# =============================================================================
# Publishing Configuration (Android)
# =============================================================================

# Maven Central / Sonatype OSSRH credentials (if publishing to Maven Central)
SONATYPE_USERNAME=your_sonatype_username
SONATYPE_PASSWORD=your_sonatype_password

# GPG signing configuration (for Maven Central)
signing.keyId=your_gpg_key_id
signing.password=your_gpg_key_password
signing.secretKeyRingFile=/path/to/your/secring.gpg

# =============================================================================
# Publishing Configuration (iOS)
# =============================================================================

# CocoaPods Trunk is authenticated via `pod trunk register`
# No environment variables needed - authentication is done once per machine

# =============================================================================
# Development
# =============================================================================

# Android SDK location (auto-generated in android/local.properties)
# ANDROID_SDK_ROOT=/path/to/android/sdk

# Debug mode for verbose logging
DEBUG=false
